<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestAutomationPractice" parallel="classes" thread-count="5">
  <listeners>
    <listener class-name="Listeners.ListenersForReportListener"/>
    <listener class-name="ExecutionListener"/>
    <listener class-name="Listeners.ReportListener"/>
  </listeners>
  <test name="AllTestCases" thread-count="5" >
    <classes>
      <class name="tests.LoginExistingUserAsNew"/>
      <class name="tests.LoginExistingUserThenLogOut"/>
      <class name="tests.LoginIncorrectUser"/>
      <class name="tests.LoginNewUserThenDelete"/>
      <class name="tests.SendContactInfo"/>
      <class name="tests.ThisTestIsFlaky"/>
      <class name="tests.VerifyProductPageDetails"/>
      <class name="tests.VerifyProductsAddedToCart"/>
      <class name="tests.VerifySearchFeature"/>
      <class name="tests.VerifySubscribeFooter"/>
      <class name="tests.VerifyTestCasesHeaderButton"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
